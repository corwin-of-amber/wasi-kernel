{
  "name": "wasi-kernel",
  "version": "0.1.6",
  "description": "A POSIX-like runtime environment on top of Wasmer-JS",
  "browser": "src/kernel/index.ts",
  "main": "lib/kernel/index.js",
  "bin": {
    "wasi-kit": "scripts/kit.js"
  },
  "side-effects": false,
  "scripts": {
    "autogen": "node scripts/autogen-delegation.js",
    "build": "webpack --mode production && tsc --emitDeclarationOnly --outDir lib",
    "build:app": "parcel build shell.html",
    "dist": "rm -rf dist lib && npm run build && npm pack"
  },
  "files": [
    "dist",
    "include",
    "lib",
    "scripts",
    "src",
    "tsconfig.json"
  ],
  "dependencies": {
    "@wasmer/wasi": "^0.12.0",
    "@wasmer/wasm-transformer": "^0.12.0",
    "@wasmer/wasmfs": "^0.12.0",
    "assert": "^2.0.0",
    "buffer": "^4.9.2",
    "memfs": "3.0.4"
  },
  "devDependencies": {
    "@types/node": "^13.1.1",
    "events": "^3.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "ts-loader": "^9.2.5",
    "typescript": "^4.2.4",
    "wabt": "^1.0.23",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0"
  },
  "author": "",
  "license": "ISC",
  "browserslist": [
    "Chrome 78"
  ]
}
