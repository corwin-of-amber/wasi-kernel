{
  "name": "wasi-kernel",
  "version": "0.2.0",
  "description": "A POSIX-like runtime environment on top of Wasmer-JS",
  "browser": "src/kernel/index.ts",
  "main": "lib/kernel/index.js",
  "bin": {
    "wasi-kit": "scripts/kit.js"
  },
  "exports": {
    ".": {
      "types": "./lib/kernel/index.d.ts",
      "import": "./lib/kernel/index.js"
    },
    "./cjs": "./lib/kernel/index.cjs.js",
    "./bits/tty": "./src/kernel/bits/tty.ts",
    "./services": "./src/kernel/services/index.ts",
    "./extra/shell": "./src/extra/shell/shell.ts"
  },
  "side-effects": false,
  "scripts": {
    "autogen": "node scripts/autogen-delegation.js",
    "build": "webpack --mode production && tsc --emitDeclarationOnly",
    "build:app": "parcel build shell.html",
    "dist": "rm -rf dist lib && npm run build && npm pack"
  },
  "files": [
    "dist",
    "include",
    "lib",
    "scripts",
    "src",
    "tsconfig.json"
  ],
  "dependencies": {
    "@wasmer/wasi": "^0.12.0",
    "@wasmer/wasm-transformer": "^0.12.0",
    "@wasmer/wasmfs": "^0.12.0",
    "assert": "^2.0.0",
    "browser-or-node": "^2.0.0",
    "concat-stream": "^2.0.0",
    "memfs": "3.0.4",
    "tar-stream": "^2.2.0"
  },
  "devDependencies": {
    "@types/concat-stream": "^2.0.0",
    "@types/node": "^13.1.1",
    "@types/tar-stream": "^2.2.2",
    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "wabt": "^1.0.23",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  },
  "author": "",
  "license": "ISC",
  "browserslist": [
    "Chrome 78"
  ]
}
