{
  "name": "wasi-kernel",
  "version": "0.1.1-rc2",
  "description": "A POSIX-like runtime environment on top of Wasmer-JS",
  "browser": "src/kernel/index.ts",
  "main": "lib/kernel/index.js",
  "bin": {
    "wasi-kit": "scripts/kit.js"
  },
  "scripts": {
    "build": "parcel build src/kernel/worker.ts && ./build-node.sh",
    "build:app": "parcel build shell.html",
    "pack": "rm -rf dist lib && npm run build && npm pack"
  },
  "files": [
      "dist",
      "include",
      "lib",
      "scripts",
      "src",
      "tsconfig.json"
  ],
  "dependencies": {
    "@wasmer/wasi": "^0.11.2",
    "@wasmer/wasm-transformer": "^0.11.2",
    "@wasmer/wasmfs": "^0.11.2",
    "assert": "^2.0.0",
    "buffer": "^5.4.3",
    "memfs": "^3.0.4"
  },
  "devDependencies": {
    "@types/node": "^13.1.1",
    "typescript": "^4.0.2"
  },
  "author": "",
  "license": "ISC",
  "browserslist": [
    "Chrome 78"
  ]
}
